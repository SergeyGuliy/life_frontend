{
  "name": "life_frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "test:dev": "http-server dist -p 9999 -c 1 --push-state"
  },
  "dependencies": {
    "@vue/composition-api": "^1.1.5",
    "core-js": "^3.6.5",
    "get-blob-duration": "^1.2.0",
    "lodash": "^4.17.21",
    "moment": "latest",
    "register-service-worker": "^1.7.1",
    "socket.io-client": "^4.1.2",
    "v-click-outside": "^3.1.2",
    "v-mask": "^2.2.4",
    "vue": "^2.6.11",
    "vue-bus": "^1.2.1",
    "vue-debounce": "^2.6.0",
    "vue-i18n": "^8.24.1",
    "vue-long-click": "0.0.4",
    "vue-moment": "^4.1.0",
    "vue-notification": "^1.3.20",
    "vue-router": "^3.2.0",
    "vue-socket.io": "^3.0.10",
    "vue-socket.io-extended": "^4.2.0",
    "vuetify": "^2.4.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@mdi/font": "^2.1.99",
    "@vue/cli-plugin-babel": "^4.5.0",
    "@vue/cli-plugin-eslint": "^4.5.0",
    "@vue/cli-plugin-pwa": "^4.5.0",
    "@vue/cli-plugin-router": "^4.5.0",
    "@vue/cli-plugin-unit-jest": "^4.5.13",
    "@vue/cli-plugin-vuex": "^4.5.0",
    "@vue/cli-service": "^4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^1.0.3",
    "axios": "^0.18.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "node-sass": "^4.12.0",
    "prettier": "^1.19.1",
    "resize-observer-polyfill": "^1.5.1",
    "sass": "^1.32.0",
    "sass-loader": "^10.0.0",
    "socket.io": "^4.0.0",
    "vue-cli-plugin-axios": "0.0.4",
    "vue-cli-plugin-vuetify": "^2.2.2",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.7.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint . --fix",
      "git add"
    ],
    "src/**/*.scss": [
      "stylelint --syntax scss --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/prettier"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
